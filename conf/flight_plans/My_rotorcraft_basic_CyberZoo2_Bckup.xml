<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="41" ground_alt="39.5" lat0="60.2806185" lon0="5.2225626" max_dist_from_home="80" name="Rotorcraft Basic (Enac)" security_height="4">
  <header>
	#include "autopilot.h"
    #include "subsystems/ahrs.h"
    #include "subsystems/electrical.h"
    #include "subsystems/datalink/datalink.h"
	#include "modules/manan_test/manan_test.h"
	#include "modules/manan_test/visrl.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0.0" y="0.0"/>
    <waypoint name="CLIMB" x="0.0" y="2.5"/>
    <waypoint lat="60.2949" lon="5.2251" name="TEMP"/>
    <waypoint lat="60.2949" lon="5.2251045" name="CURPOS"/>
    <waypoint lat="60.295034" lon="5.225104" name="TESTWP"/>
    <waypoint x="0" y="0" name="STDBY"/>
    <waypoint lat="60.2948404" lon="5.2250930" name="GOAL"/>
    <waypoint lat="60.2952" lon="5.2256" name="HeightRef"/>
    <waypoint lat="60.2950532" lon="5.2245440" name="p1"/>
    <waypoint lat="60.2950305" lon="5.2255602" name="p2"/>
    <waypoint lat="60.2947578" lon="5.2255819" name="p3"/>
    <waypoint lat="60.2947389" lon="5.2245349" name="p4"/>
    <waypoint x="-4" y="4" name="SZ1"/>
    <waypoint x="4" y="4" name="SZ2"/>
    <waypoint x="4" y="-4" name="SZ3"/>
    <waypoint x="-4" y="-4" name="SZ4"/>
    <waypoint x="0.25" y="0.25" name="BoundaryChecker"/>
    <waypoint height="12." name="CAM" x="-20" y="-50"/>
    <waypoint name="XYFINDER" x="30" y="-30"/>
    <waypoint name="TD" x="5.6" y="-10.9"/>
  </waypoints>
  <sectors>
    <sector color="green" name="MyWorld">
      <corner name="SZ1"/>
      <corner name="SZ2"/>
      <corner name="SZ3"/>
      <corner name="SZ4"/>
    </sector>
  </sectors>
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call fun="NavSetGroundReferenceHere()"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="MyTakeoff">
      <call fun="NavResurrect()"/>
      <exception cond="stateGetPositionEnu_f()->z > 2.0" deroute="Standby"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="nav_climb_vspeed" vmode="climb" wp="CLIMB"/>
      <deroute block="Standby"/>
    </block>
    <block name="Standby" strip_button="Standby" strip_icon="home.png">
      <stay wp="STDBY"/>
    </block>
    <block name="YawRight">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="increase_nav_heading_deg(&nav_heading, 30)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="YawLeft">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="increase_nav_heading_deg(&nav_heading, -30)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="For">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="moveWaypointForwards(WP_GOAL,2)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="Bac">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="moveWaypointForwards(WP_GOAL,-2)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="TESTBLOCK">
      <call fun="setHeadingNorth()"/>
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <go wp="GOAL"/>
      <stay wp="GOAL"/>
    </block>

    <block name="GoToGoal">
      <go wp="GOAL"/>
      <stay wp="GOAL"/>
    </block>
    <block name="GoToTemp">
      <go wp="TEMP"/>
      <stay wp="TEMP"/>
    </block>
    <block name="3Grids">
        <call fun="cv_3grids()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="InitQdict">
        <call fun="init_qdict()"/>
      <stay wp="GOAL"/>
    </block>
    <!--
    <block name="AppendQdict">
        <call fun="append_qdict()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="PrintQdict">
        <call fun="print_qdict()"/>
      <stay wp="GOAL"/>
    </block>
-->
    <block name="SizeQdict">
        <call fun="size_qdict()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="PrintCVArrs">
        <call fun="cv_3grids()"/>
        <call fun="print_cvarrs()"/>
      <stay wp="GOAL"/>
    </block>
    <!--
    <block name="SetupEpisode">
      <go wp="HOME"/>
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="rl_reset_episode()"/>
    </block>
    <block name="SetupEpisode2">
      <call fun="nav_set_heading_towards_waypoint(WP_TESTWP)"/>
      <stay wp="GOAL"/>
    </block>
-->
    <block name="SetupEpisode">
        <!-- <go wp="HOME"/> -->
      <call fun="NavSetWaypointHere(WP_CURPOS)"/>
      <go from="CURPOS" wp="HOME" hmode="route" approaching_time="0"/>
      <call fun="setHeadingNorth()"/>
      <stay wp="HOME" until="stage_time>5"/>
      <call fun="rl_reset_episode()"/>
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <!-- <call fun="setAltToWp(WP_GOAL,WP_HeightRef)"/> -->
      <call fun="setAltToWp(WP_GOAL,WP_HOME)"/>
      <!--
      <for var="i" from="1" to="8">
        <heading alt="WaypointAlt(WP_GOAL)" course="45 * $i" until="stage_time > 1.2"/>
      </for>
        -->
      <stay wp="GOAL" until="stage_time>1"/>
    </block>
    <block name="InitRL">
      <call fun="rl_init()"/>
      <stay until="stage_time>1" wp="GOAL"/>
    </block>
    <block name="DoOneStep">
      <while cond="!rl_isterminal">
        <call fun="rl_set_cur()"/>
        <call fun="rl_take_cur_action()"/>
        <stay until="stage_time>1" wp="GOAL"/>
        <call fun="rl_set_nxt()"/>
        <!-- <call fun="rl_update_qtab()"/> -->
        <call fun="rl_update_qdict()"/>
        <call fun="rl_check_terminal()"/>
      </while>
    </block>
    <block name="EndEp">
      <call fun="rl_write_episode_log()"/>
	  <exception cond="episodes_simulated > 500" deroute="EndRl"/>
      <!-- <stay wp="GOAL" until="stage_time > 1"/> -->
	  <deroute block="SetupEpisode"/>
    </block>
    <block name="EndRl">
      <stay wp="GOAL"/>
      <call fun="rl_write_episode_log()"/>
      <call fun="print_qdict()"/>
    </block>
    <block name="WriteEpLog">
      <call fun="rl_write_episode_log()"/>
    </block>
    <block name="PrintQtab">
      <call fun="rl_print_qtab()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="PrintQDict">
      <call fun="print_qdict()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="ReadQtab">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="rl_read_qtab()"/>
      <stay wp="TEMP"/>
    </block>
    <block name="WriteQtab">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="rl_write_qtab()"/>
      <stay wp="TEMP"/>
    </block>
    <block name="WriteQDict">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="write_qdict()"/>
      <stay wp="TEMP"/>
    </block>
    <block name="ReadQDict">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="load_qdict()"/>
      <stay wp="TEMP"/>
    </block>
    <!--
    -->
    <block name="land here" strip_button="Land Here" strip_icon="land-right.png">
      <call fun="NavSetWaypointHere(WP_TD)"/>
    </block>
    <block name="land">
      <go wp="TD"/>
    </block>
    <block name="flare">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="landed"/>
      <call fun="NavStartDetectGround()"/>
      <stay climb="nav_descend_vspeed" vmode="climb" wp="TD"/>
    </block>
    <block name="landed">
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
