<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="57" ground_alt="49.03" lat0="60.2948404" lon0="5.2250930" max_dist_from_home="80" name="Rotorcraft Basic (Enac)" security_height="4">
  <header>
	#include "autopilot.h"
    #include "subsystems/ahrs.h"
    #include "subsystems/electrical.h"
    #include "subsystems/datalink/datalink.h"
	#include "modules/manan_test/manan_test.h"
	#include "modules/manan_test/visrl.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="-149.1" y="-1279.4"/>
    <waypoint name="CLIMB" x="-66.6" y="-1305.4"/>
    <waypoint lat="60.283341" lon="5.223122" name="TEMP"/>
    <waypoint lat="60.283363" lon="5.220011" name="CURPOS"/>
    <waypoint lat="60.283206" lon="5.219986" name="TESTWP"/>
    <waypoint lat="60.283286" lon="5.220007" name="STDBY"/>
    <waypoint lat="60.283227" lon="5.223096" name="GOAL"/>
    <waypoint lat="60.283050" lon="5.223086" name="HeightRef"/>
    <waypoint lat="60.283448" lon="5.221132" name="p1"/>
    <waypoint lat="60.283478" lon="5.222440" name="p2"/>
    <waypoint lat="60.283141" lon="5.222382" name="p3"/>
    <waypoint lat="60.283016" lon="5.221464" name="p4"/>
    <waypoint lat="60.283556" lon="5.220656" name="SZ1"/>
    <waypoint lat="60.283686" lon="5.222275" name="SZ2"/>
    <waypoint lat="60.282940" lon="5.222731" name="SZ3"/>
    <waypoint lat="60.282831" lon="5.220912" name="SZ4"/>
    <waypoint lat="60.283443" lon="5.220020" name="BoundaryChecker"/>
    <waypoint name="CAM" x="-66.1" y="-1251.8"/>
    <waypoint name="XYFINDER" x="-65.3" y="-1262.6"/>
    <waypoint name="TD" x="-64.9" y="-1272.7"/>
  </waypoints>
  <sectors>
    <sector color="green" name="MyWorld">
      <corner name="SZ1"/>
      <corner name="SZ2"/>
      <corner name="SZ3"/>
      <corner name="SZ4"/>
    </sector>
  </sectors>
  <blocks>
    <block name="Wait GPS">
      <call fun="NavKillThrottle()"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call fun="NavSetGroundReferenceHere()"/>
    </block>
    <block name="Holding point">
      <call fun="NavKillThrottle()"/>
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
    <block name="MyTakeoff">
      <call fun="NavResurrect()"/>
      <exception cond="stateGetPositionEnu_f()->z > 2.0" deroute="Standby"/>
      <call fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay climb="nav_climb_vspeed" vmode="climb" wp="CLIMB"/>
      <deroute block="Standby"/>
    </block>
    <block name="Standby" strip_button="Standby" strip_icon="home.png">
      <stay wp="STDBY"/>
    </block>
    <block name="YawRight">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="increase_nav_heading_deg(&nav_heading, 30)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="YawLeft">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="increase_nav_heading_deg(&nav_heading, -30)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="For">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="moveWaypointForwards(WP_GOAL,2)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="Bac">
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="moveWaypointForwards(WP_GOAL,-2)"/>
      <stay wp="GOAL"/>
    </block>
    <block name="TESTBLOCK">
      <call fun="setHeadingNorth()"/>
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <go wp="GOAL"/>
      <stay wp="GOAL"/>
    </block>
    <block name="GoToGoal">
      <go wp="GOAL"/>
      <stay wp="GOAL"/>
    </block>
    <block name="GoToTemp">
      <go wp="TEMP"/>
      <stay wp="TEMP"/>
    </block>
    <block name="3Grids">
      <call fun="cv_3grids()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="InitQdict">
      <call fun="init_qdict()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="SizeQdict">
      <call fun="size_qdict()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="PrintCVArrs">
      <call fun="cv_3grids()"/>
      <call fun="print_cvarrs()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="SetupEpisode">
      <call fun="NavSetWaypointHere(WP_CURPOS)"/>
      <go approaching_time="0" from="CURPOS" hmode="route" wp="HOME"/>
      <call fun="setHeadingNorth()"/>
      <stay until="stage_time>5" wp="HOME"/>
      <call fun="rl_reset_episode()"/>
      <call fun="NavSetWaypointHere(WP_GOAL)"/>
      <call fun="setAltToWp(WP_GOAL,WP_HOME)"/>
      <stay until="stage_time>1" wp="GOAL"/>
    </block>
    <block name="InitRL">
      <call fun="rl_init()"/>
      <stay until="stage_time>1" wp="GOAL"/>
    </block>
    <block name="DoOneStep">
      <while cond="!rl_isterminal">
        <call fun="rl_set_cur()"/>
        <call fun="rl_take_cur_action()"/>
        <stay until="stage_time>1" wp="GOAL"/>
        <call fun="rl_set_nxt()"/>
        <call fun="rl_update_qdict()"/>
        <call fun="rl_check_terminal()"/>
      </while>
    </block>
    <block name="EndEp">
      <call fun="rl_write_episode_log()"/>
      <exception cond="episodes_simulated > 500" deroute="EndRl"/>
      <deroute block="SetupEpisode"/>
    </block>
    <block name="EndRl">
      <stay wp="GOAL"/>
      <call fun="rl_write_episode_log()"/>
      <call fun="print_qdict()"/>
    </block>
    <block name="WriteEpLog">
      <call fun="rl_write_episode_log()"/>
    </block>
    <block name="PrintQtab">
      <call fun="rl_print_qtab()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="PrintQDict">
      <call fun="print_qdict()"/>
      <stay wp="GOAL"/>
    </block>
    <block name="ReadQtab">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="rl_read_qtab()"/>
      <stay wp="TEMP"/>
    </block>
    <block name="WriteQtab">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="rl_write_qtab()"/>
      <stay wp="TEMP"/>
    </block>
    <block name="WriteQDict">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="write_qdict()"/>
      <stay wp="TEMP"/>
    </block>
    <block name="ReadQDict">
      <call fun="NavSetWaypointHere(WP_TEMP)"/>
      <call fun="load_qdict()"/>
      <stay wp="TEMP"/>
    </block>
    <block name="land here" strip_button="Land Here" strip_icon="land-right.png">
      <call fun="NavSetWaypointHere(WP_TD)"/>
    </block>
    <block name="land">
      <go wp="TD"/>
    </block>
    <block name="flare">
      <exception cond="NavDetectGround()" deroute="Holding point"/>
      <exception cond="!nav_is_in_flight()" deroute="landed"/>
      <call fun="NavStartDetectGround()"/>
      <stay climb="nav_descend_vspeed" vmode="climb" wp="TD"/>
    </block>
    <block name="landed">
      <attitude pitch="0" roll="0" throttle="0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
