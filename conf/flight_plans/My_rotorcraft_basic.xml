      <oval p1="p1" p2="p2" radius="-1"/>
    </block>
    <block name="test yaw">
      <go wp="p1"/>
      <for var="i" from="1" to="16">
        <heading alt="WaypointAlt(WP_p1)" course="90 * $i" until="stage_time > 3"/>
      </for>
      <deroute block="Standby"/>
    </block>
