        <!--
    <block name="StayGoal">

        <exception cond="block_time > 3" deroute="MoveGoalForward"/>

	<exception cond="!isSafeToGoForwards" deroute="TurnAround"/>
	<exception cond="!InsideObstacleZone(GetPosX(),GetPosY())" deroute="GoBackToZone_TurnAround"/>

    </block>
	-->
    <block name="For">
        <call fun="moveWaypointForwards(WP_GOAL,5)"/>
        <go wp="GOAL"/>
        <stay wp="GOAL"/>
    </block>
    <block name="Bac">
        <call fun="moveWaypointBackwards(WP_GOAL,5)"/>
        <go wp="GOAL"/>
        <stay wp="GOAL"/>
    </block>
    <block name="Lef">
        <call fun="moveWaypointLeftwards(WP_GOAL,5)"/>
        <go wp="GOAL"/>
        <stay wp="GOAL"/>
    </block>
    <block name="Rig">
        <call fun="moveWaypointRightwards(WP_GOAL,5)"/>
        <go wp="GOAL"/>
        <stay wp="GOAL"/>
    </block>

    <block name="YawRight">
        <call fun="NavSetWaypointHere(WP_STDBY)"/>
        <call fun="increase_nav_heading_deg(&nav_heading, 45)"/>
        <stay wp="STDBY"/>
    </block>
    <block name="YawLeft">
        <call fun="NavSetWaypointHere(WP_STDBY)"/>
        <call fun="increase_nav_heading_deg(&nav_heading, -45)"/>
        <stay wp="STDBY"/>
    </block>
<!--
    <block name="TEST">
        <call fun="a()"/>
    </block>
-->
    <block name="PrintHeadingAndReds">
        <call fun="NavSetWaypointHere(WP_STDBY)"/>
        <!--call fun="printme()"/-->
        <call fun="printheading(&nav_heading)"/>
        <call fun="runcurltomem()"/>
        <stay wp="STDBY"/>
    </block>

    <block name="TurnAndLook">
        <call fun="NavSetWaypointHere(WP_STDBY)"/>
        <call fun="increase_nav_heading_deg(&nav_heading, 45)"/>
        <stay wp="STDBY" until="stage_time>3"/>
        <call fun="printheading(&nav_heading)"/>
        <call fun="runcurltomem()"/>
        <stay wp="STDBY"/>
    </block>

    <block name="NAV_TEST">
        <call fun="nav_set_heading_deg(45)"/>
        <call fun="printheading(&GetCurHeading())"/>
        <call fun="printheading(&nav_heading)"/>
        <stay wp="STDBY" until="stage_time>3"/>
        <call fun="printheading(&GetCurHeading())"/>
        <stay wp="STDBY"/>
    </block>

    <block name="PRINT_HEADING">
        <call fun="printheading(&GetCurHeading())"/>
        <stay wp="STDBY"/>
    </block>

    <block name="Ballerina">
        <call fun="printheading(&nav_heading)"/>
        <call fun="NavSetWaypointHere(WP_STDBY)"/>
        <for var="i" from="0" to="35" >
	    <!--call fun="nav_set_heading_deg((i+1)*10)"/-->
	    <heading alt="WaypointAlt(WP_STDBY)" course="($i+1)*10" until="stage_time>2"/>
            <call fun="update_redsatheading()"/>
            <call fun="printheading(&GetCurHeading())"/>
            <stay wp="STDBY" until="stage_time>2"/>
	    <!--call fun="printheading(&GetCurHeading())"/-->
        </for>
        <stay wp="STDBY"/>
    </block>

    <block name="Print_redsatheading">
        <call fun="printredsatheading()"/>
        <stay wp="STDBY"/>
    </block>


